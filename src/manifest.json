{
    "name": "__MSG_appTitle__",
    "manifest_version": 3,
    "version": "2.0.0",
    "description": "__MSG_appDescription__",
    "permissions": ["bookmarks", "tabs"],
    "background": {
        "service_worker": "./background/main.js"
    },
    "action": {
        "default_title": "__MSG_actionTitle__",
        "default_popup": "./popup.html",
        "default_icon": {
            "16": "./assets/images/icon16_tag.png",
            "32": "./assets/images/icon32_tag.png",
            "48": "./assets/images/icon48_tag.png",
            "64": "./assets/images/icon64_tag.png",
            "128": "./assets/images/icon128_tag.png"
        }
    },
    "icons": {
        "16": "./assets/images/icon16_tag.png",
        "32": "./assets/images/icon32_tag.png",
        "48": "./assets/images/icon48_tag.png",
        "64": "./assets/images/icon64_tag.png",
        "128": "./assets/images/icon128_tag.png"
    },
    "default_locale": "zh_CN"
}